<center><h2><strong>Restaurants:</strong></h2>
<br>
<h6><%= link_to "Create new restaurant.", new_restaurant_path %></h6>
<br>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Phone Number</th>
      <th>Category</th>
    </tr>
  </thead>
  <tbody>
    <% @restaurants.each do |restaurant| %>
      <tr>
        <td><%= restaurant.name %></td>
        <td><%= restaurant.address %></td>
        <td><%= restaurant.phone_number %></td>
        <td><%= restaurant.category %></td>
        <td><%= link_to "details", restaurant_path(restaurant) %></td>
        <td><%= link_to "add review", new_restaurant_review_path(restaurant) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</center>
